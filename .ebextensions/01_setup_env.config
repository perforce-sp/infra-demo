commands:
  setvars:
    command: |
      /opt/elasticbeanstalk/bin/get-config environment | jq -r 'to_entries | .[] | "export \(.key)=\"\(.value)\""' > /etc/profile.d/sh.local
      sudo amazon-linux-extras install java-openjdk11 -y
      sudo yum -y install java-1.8.0-openjdk

packages:
    yum:
        jq: []